{
  "name": "carefree-react-native",
  "description": "练习 react native ,并尝试封装常用的组件",
  "version": "0.0.44",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --port 8082",
    "test": "jest",
    "postpublish": "git clean -fd",
    "release": "npm run build && npm publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,json}'",
    "start:doc": "dumi dev",
    "docs:build": "dumi build"
  },
  "dependencies": {
    "@babel/runtime": "7.15.3",
    "carefree-utils": "1.0.17",
    "rc-field-form": "1.21.2",
    "react-native-svg": "12.1.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.1",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "dumi": "^1.1.30",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.3.2",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.4.4",
    "yorkie": "2.0.0"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}